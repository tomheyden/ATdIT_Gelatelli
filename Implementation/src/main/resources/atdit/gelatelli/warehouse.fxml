<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="atdit.gelatelli.controllers.WarehouseController">
   <children>
      <Label layoutX="27.0" layoutY="54.0" prefHeight="17.0" prefWidth="98.0" text="Warehouse" />
      <ComboBox layoutX="131.0" layoutY="50.0" prefHeight="26.0" prefWidth="98.0" promptText="Filter" />
      <Label layoutX="328.0" layoutY="55.0" prefHeight="17.0" prefWidth="108.0" text="Incoming goods">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Label>
      <ComboBox fx:id="goodsComboBox" layoutX="328.0" layoutY="81.0" prefWidth="150.0" promptText="Choose good" />
      <ChoiceBox fx:id="amountComboBox" layoutX="328.0" layoutY="140.0" prefHeight="26.0" prefWidth="98.0" />
      <Label layoutX="328.0" layoutY="117.0" prefHeight="17.0" prefWidth="108.0" text="Amount" />
      <ChoiceBox fx:id="unitComboBox" layoutX="328.0" layoutY="204.0" prefHeight="26.0" prefWidth="98.0" />
      <Label layoutX="328.0" layoutY="181.0" prefHeight="17.0" prefWidth="108.0" text="Unit" />
      <DatePicker fx:id="expiryDatePicker" layoutX="328.0" layoutY="255.0" prefWidth="150.0" promptText="Expiry Date" />
      <Separator layoutX="280.0" layoutY="1.0" orientation="VERTICAL" prefHeight="400.0" prefWidth="20.0" />
      <Label fx:id="doneLabel" layoutX="328.0" layoutY="340.0" prefHeight="17.0" prefWidth="39.0" text="Done" />
      <ListView fx:id="warehouseListView" layoutX="25.0" layoutY="98.0" prefHeight="200.0" prefWidth="200.0" />
      <Button fx:id="updateButton" layoutX="27.0" layoutY="314.0" mnemonicParsing="false" onAction="#handleUpdateButtonAction" prefHeight="26.0" prefWidth="98.0" text="Update" />
      <Button fx:id="goBackButton" layoutX="-1.0" layoutY="-1.0" mnemonicParsing="false" onAction="#handleGoBackButtonAction" prefHeight="26.0" prefWidth="98.0" text="&lt; Go Back" />
      <Button fx:id="insertButton" layoutX="328.0" layoutY="301.0" mnemonicParsing="false" onAction="#handleInsertButton" prefHeight="26.0" prefWidth="98.0" text="Insert" />
      <Label fx:id="hintLabel" layoutX="29.0" layoutY="349.0" prefHeight="17.0" prefWidth="200.0" text="" />
   </children>
</AnchorPane>
